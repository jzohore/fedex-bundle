{
    "type": "symfony-bundle",
    "name": "junior-dev/fedex-bundle",
    "description": "Symfony bundle for integrating with the FedEx REST API (OAuth2, shipping, rates, tracking).",
    "keywords": [
        "symfony",
        "bundle",
        "fedex",
        "shipping",
        "label",
        "tracking",
        "logistics",
        "carrier",
        "api",
        "rest",
        "oauth2",
        "fedex api",
        "fedex shipping",
        "fedex integration",
        "fedex symfony"
    ],
    "license": "MIT",
    "minimum-stability": "stable",
    "prefer-stable": true,
    "authors": [
        {
            "name": "ZohorÃ© Junior",
            "role": "Author",
            "email": "zohorejuniorpro@gmail.com"
        }
    ],
    "require": {
        "php": ">=8.1.0",
        "symfony/stimulus-bundle": "^2.9.1",
        "twig/extra-bundle": "^2.12|^3.0",
        "symfony/twig-bundle": "^6.1|^7.0",
        "symfony/http-client": "^6.1|^7.0"
    },
    "require-dev": {
        "phpstan/phpstan": "^2.1.17",
        "phpunit/phpunit": "^9.5",
        "friendsofphp/php-cs-fixer": "^3.1",
        "symfony/browser-kit": "^6.1|^7.0",
        "symfony/framework-bundle": "^6.1|^7.0",
        "symfony/asset-mapper": "^6.3|^7.0",
        "friendsoftwig/twigcs": "^6.4",
        "dg/bypass-finals": "^1.6"
    },
    "autoload": {
        "psr-4": {
            "SonnyDev\\FedexBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SonnyDev\\FedexBundle\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "phpcsfixer": "./vendor/bin/php-cs-fixer fix",
        "phpcsfixer-lint": "./vendor/bin/php-cs-fixer fix --dry-run --diff",
        "twig-cs-lint": "./vendor/bin/twigcs ./src/templates/",
        "phpstan": "./vendor/bin/phpstan --memory-limit=1G analyse",
        "rector":  "./vendor/bin/rector process --dry-run",
        "rector-nocache":  "./vendor/bin/rector process --dry-run --clear-cache",
        "rector-no-dry":  "./vendor/bin/rector process",
        "ci": [
            "@phpcsfixer-lint",
            "@phpstan",
            "@twig-cs-lint"
        ]
    },
    "config": {
        "allow-plugins": {
            "phpstan/extension-installer": true
        }
    }
}
